@model Empleado
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Crear Empleado</h1>

<form method="post" asp-action="Create">

<label>Nombre Empleado</label>
    <input asp-for="NombreEmpleado" />
    <br />
    <label>Sueldo</label>
    <input asp-for="sueldo" />
    <br />
    <label>Liquidacion Total</label>
    <input asp-for="LiquidacionTotal" />
    <br />
    <label>Decimo Tercer Sueldo</label>
    <input asp-for="DicemoTercerSueldo" />
    <br />
    <label>Rol De Pagos </label>
    <input asp-for="RoldePagos" />
    <br />
    <input type="submit" value="Crear" />
   </form>

